// 팝업 익스텐션
{
    "manifest_version": 2,
     
    "name": "get_source",
    "description": "print html code.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "cookie.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}


