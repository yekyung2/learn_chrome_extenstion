{
    "manifest_version": 2,
     
    "name": "Hello_World",
    "description": "Print Hello, World! message",
    "version": "1.0",
     
    "browser_action": {
        "default_icon": "cookie.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
}